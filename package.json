{
  "name": "blog",
  "version": "0.0.1",
  "description": "blog",
  "scripts": {
    "start-dev": "NODE_ENV=development NODE_MODE=full nodemon backend/index.js --ignore 'frontend/*.js' --exec babel-node --presets es2015,stage-2",
    "build": "webpack --progress --colors --display-error-details --config webpack.dev.js"
  },
  "dependencies": {
    "autoprefixer": "6.7.7",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.2",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.23.0",
    "compression-webpack-plugin": "0.3.2",
    "connect-mongo": "^1.3.2",
    "css-loader": "0.26.4",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-react": "6.10.3",
    "express-session": "^1.15.3",
    "morgan": "^1.8.2",
    "postcss-loader": "1.3.3",
    "precss": "1.4.0",
    "react-hot-loader": "1.3.1",
    "serve-favicon": "^2.4.3",
    "static-site-generator-webpack-plugin": "3.4.1",
    "style-loader": "0.13.2",
    "webpack": "2.4.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.18.0"
  },
  "devDependencies": {
    "async": "2.3.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "1.17.1",
    "co": "4.6.0",
    "cookie-parser": "^1.4.3",
    "elasticsearch": "^13.0.1",
    "eslint-plugin-import": "^2.3.0",
    "express": "4.15.2",
    "influx": "5.0.7",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "^3.2.2",
    "moment": "2.18.1",
    "mongoose": "4.9.5",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "prop-types": "15.5.8",
    "react": "^15.5.4",
    "react-bootstrap": "0.30.9",
    "react-copy-to-clipboard": "^5.0.0",
    "react-datetime": "2.8.9",
    "react-dom": "15.5.4",
    "react-google-charts": "1.5.3",
    "react-redux": "5.0.4",
    "react-router": "3.0.5",
    "redux": "3.6.0",
    "redux-logger": "2.10.2",
    "redux-thunk": "2.2.0",
    "request": "2.81.0",
    "whatwg-fetch": "2.0.3"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    },
    "rules": {
      "quotes": 0,
      "no-console": 1,
      "no-debugger": 1,
      "no-var": 1,
      "semi": [
        1,
        "always"
      ],
      "no-trailing-spaces": 0,
      "eol-last": 0,
      "no-underscore-dangle": 0,
      "no-alert": 0,
      "no-lone-blocks": 0,
      "jsx-quotes": 1,
      "react/display-name": [
        1,
        {
          "ignoreTranspilerName": false
        }
      ],
      "react/forbid-prop-types": [
        1,
        {
          "forbid": [
            "any"
          ]
        }
      ],
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 0,
      "react/jsx-curly-spacing": 1,
      "react/jsx-indent-props": 0,
      "react/jsx-key": 1,
      "react/jsx-max-props-per-line": 0,
      "react/jsx-no-bind": 0,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-literals": 0,
      "react/jsx-no-undef": 1,
      "react/jsx-pascal-case": 1,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/jsx-wrap-multilines": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-direct-mutation-state": 1,
      "react/no-multi-comp": 1,
      "react/no-set-state": 0,
      "react/no-unknown-property": 1,
      "react/prefer-es6-class": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "import/extensions": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1
    },
    "globals": {}
  }
}
